<!-- Loop over each object in the projects array -->
{{#each blogs as |blog| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
      
      <a href="/blog/{{blog.id}}">{{blog.post_title}}</a>
    </h2>
    
    <p>Created by {{blog.user_id}} on {{date_time}}</p>
  </div>
  <div class="col-md-7">
    <p>
      {{blog.contents}}
    </p>
  </div>
</div>
{{/each}}
